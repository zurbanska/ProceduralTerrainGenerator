            <ui:Slider picking-mode="Ignore" label="Green" value="42" high-value="1" name="green-slider" show-input-field="true" class="slim-label" />
            <ui:Slider picking-mode="Ignore" label="Blue" value="42" high-value="1" name="blue-slider" show-input-field="true" class="slim-label" />
