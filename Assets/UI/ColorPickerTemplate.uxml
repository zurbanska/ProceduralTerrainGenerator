<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI/style.uss?fileID=7433441132597879392&amp;guid=e712a1d83a1bcfd45b5422f17e22f323&amp;type=3#style" />
    <ui:VisualElement style="flex-grow: 1; background-color: rgba(32, 32, 32, 0); height: 200px;">
        <ui:Label tabindex="-1" text="Adjust color" parse-escape-sequences="true" display-tooltip-when-elided="true" name="adjust-color-label" class="settings-label" style="width: 300px;" />
